{
    "name": "data-product-portal-client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:prd": "vite build --mode production",
        "dev": "npm run copy-config && vite --force",
        "preview": "npm run copy-config && npm run build && vite preview",
        "test": "vitest",
        "test:run": "vitest run",
        "check": "biome check --error-on-warnings && npm run typecheck && npm run validate-translations",
        "check:fix": "biome check --write --error-on-warnings && npm run typecheck && npm run validate-translations",
        "typecheck": "tsc --strict --noEmit",
        "extract-translations": "i18next 'src/**/*.{ts,tsx}' --config i18next-parser.config.js",
        "validate-translations": "i18next 'src/**/*.{ts,tsx}' --config i18next-parser.config.js --fail-on-update",
        "copy-config": "cp config.local.js public/config.js",
        "dep-viz": "npm run build && npx open-cli dependencies.html",
        "generate-api": "rm -rf src/store/api/services/generated/* && npx @rtk-query/codegen-openapi src/store/api/openapi-config.ts && node scripts/add-ts-nocheck.js"
    },
    "dependencies": {
        "@ant-design/charts": "^2.6.7",
        "@ant-design/colors": "^8.0.1",
        "@dagrejs/dagre": "^1.1.8",
        "@fontsource/inter": "^5.2.8",
        "@posthog/react": "^1.8.0",
        "@reduxjs/toolkit": "^2.11.2",
        "@tiptap/extension-bubble-menu": "^3.20.0",
        "@tiptap/extension-document": "^3.20.0",
        "@tiptap/extension-hard-break": "^3.20.0",
        "@tiptap/extension-link": "^3.20.0",
        "@tiptap/extension-placeholder": "^3.20.0",
        "@tiptap/extension-table": "^3.20.0",
        "@tiptap/extension-table-cell": "^3.20.0",
        "@tiptap/extension-table-header": "^3.20.0",
        "@tiptap/extension-table-row": "^3.20.0",
        "@tiptap/extension-text-style": "^3.20.0",
        "@tiptap/extension-typography": "^3.20.0",
        "@tiptap/extension-underline": "^3.20.0",
        "@tiptap/pm": "^3.0.7",
        "@tiptap/react": "^3.20.0",
        "@tiptap/starter-kit": "^3.20.0",
        "@xyflow/react": "^12.10.1",
        "antd": "^6.3.0",
        "axios": "^1.13.5",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "elkjs": "^0.11.0",
        "i18next": "^25.8.13",
        "i18next-http-backend": "^3.0.2",
        "js-sha256": "^0.11.1",
        "nuqs": "^2.8.8",
        "oidc-client-ts": "^3.4.1",
        "posthog-js": "^1.351.4",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-i18next": "^16.5.4",
        "react-oidc-context": "^3.3.0",
        "react-redux": "^9.2.0",
        "react-router": "^7.13.0",
        "react-syntax-highlighter": "^16.1.0",
        "use-debounce": "^10.1.0",
        "web-worker": "^1.5.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.4.3",
        "@rtk-query/codegen-openapi": "^2.0.0",
        "@types/node": "^25.3.0",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@vitejs/plugin-react": "^5.1.4",
        "axios-mock-adapter": "^2.1.0",
        "esbuild": "^0.27.3",
        "esbuild-runner": "^2.2.2",
        "i18next-parser": "^9.3.0",
        "msw": "^2.12.10",
        "rollup-plugin-visualizer": "^6.0.8",
        "sass": "^1.97.3",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vite-plugin-svgr": "^4.5.0",
        "vitest": "^4.0.18"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-musl": "^4.57.1"
    },
    "volta": {
        "node": "20.12.2"
    },
    "engines": {
        "node": ">= 20.9",
        "npm": ">= 10.1"
    },
    "overrides": {
        "i18next-parser": {
            "rimraf": "^4.0.0"
        },
        "zustand": {
            "use-sync-external-store": "^1.4.0"
        }
    }
}
