{
    "name": "data-product-portal-client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "npm run copy-config && vite --force",
        "build": "vite build",
        "build:prd": "vite build --mode production",
        "copy-config": "cp config.local.js public/config.js",
        "check": "biome check --error-on-warnings && npm run typecheck && npm run validate-translations",
        "check:fix": "biome check --write --error-on-warnings && npm run typecheck && npm run validate-translations",
        "start": "npm run copy-config && npm run build && vite preview",
        "extract-translations": "i18next 'src/**/*.{ts,tsx}' --config i18next-parser.config.js",
        "validate-translations": "i18next 'src/**/*.{ts,tsx}' --config i18next-parser.config.js --fail-on-update",
        "typecheck": "tsc --strict --noEmit",
        "dep-viz": "npm run build && npx open-cli dependencies.html"
    },
    "dependencies": {
        "@ant-design/colors": "^8.0.0",
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "@dagrejs/dagre": "^1.1.8",
        "@fontsource/inter": "^5.2.8",
        "@posthog/react": "^1.4.0",
        "@reduxjs/toolkit": "^2.10.1",
        "@tiptap/extension-bubble-menu": "^3.10.7",
        "@tiptap/extension-document": "^3.10.7",
        "@tiptap/extension-hard-break": "^3.10.7",
        "@tiptap/extension-link": "^3.10.7",
        "@tiptap/extension-placeholder": "^3.10.7",
        "@tiptap/extension-table": "^3.10.7",
        "@tiptap/extension-table-cell": "^3.10.7",
        "@tiptap/extension-table-header": "^3.10.7",
        "@tiptap/extension-table-row": "^3.10.7",
        "@tiptap/extension-text-style": "^3.10.7",
        "@tiptap/extension-typography": "^3.10.7",
        "@tiptap/extension-underline": "^3.10.7",
        "@tiptap/pm": "^3.0.7",
        "@tiptap/react": "^3.10.7",
        "@tiptap/starter-kit": "^3.10.7",
        "@xyflow/react": "^12.9.2",
        "antd": "^5.28.1",
        "axios": "^1.13.2",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "elkjs": "^0.11.0",
        "i18next": "^25.6.2",
        "i18next-http-backend": "^3.0.2",
        "js-sha256": "^0.11.1",
        "nuqs": "^2.7.3",
        "oidc-client-ts": "^3.4.0",
        "posthog-js": "^1.292.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-i18next": "^16.3.1",
        "react-oidc-context": "^3.3.0",
        "react-redux": "^9.2.0",
        "react-router": "^7.9.6",
        "use-debounce": "^10.0.6",
        "web-worker": "^1.5.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.5",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.4",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "axios-mock-adapter": "^2.1.0",
        "esbuild": "^0.27.0",
        "i18next-parser": "^9.3.0",
        "msw": "^2.12.1",
        "rollup-plugin-visualizer": "^6.0.5",
        "sass": "^1.94.0",
        "typescript": "^5.9.3",
        "vite": "^7.2.2",
        "vite-plugin-svgr": "^4.5.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-musl": "^4.53.2"
    },
    "volta": {
        "node": "20.12.2"
    },
    "engines": {
        "node": ">= 20.9",
        "npm": ">= 10.1"
    },
    "overrides": {
        "i18next-parser": {
            "rimraf": "^4.0.0"
        },
        "zustand": {
            "use-sync-external-store": "^1.4.0"
        }
    }
}
